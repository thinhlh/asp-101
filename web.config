<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <security>
            <requestFiltering>
                <hiddenSegments>
                    <add segment="utils" />
                    <add segment="rest" />
                    <add segment="Logs" />
                </hiddenSegments>
                <filteringRules>
                </filteringRules>
                <denyUrlSequences>
                </denyUrlSequences>
            </requestFiltering>
        </security>
        <staticContent>
            <mimeMap fileExtension=".md" mimeType="text/markdown" />
            <mimeMap fileExtension=".asp" mimeType="application/asp" />
            <mimeMap fileExtension=".aspx" mimeType="application/aspx" />
        </staticContent>
        <httpErrors errorMode="Detailed" />
    </system.webServer>
</configuration>
